    "commander": "^6.1.0",
    "debug": "4.1.1",
    "fastify": "^3.3.0",
    "fastify-cors": "^4.1.0",
    "geoip-lite": "^1.4.2",
    "hashring": "^3.2.0",
    "lru-cache": "^6.0.0",
    "pidusage": "^2.0.21",
    "promise.allsettled": "^1.0.2",
    "qs": "^6.9.4",
    "speedometer": "^1.1.0",
    "streamr-client": "^4.1.1",
    "streamr-client-protocol": "^5.0.0",
    "uWebSockets.js": "github:uNetworking/uWebSockets.js#v18.5.0",
    "uuid": "^8.3.0",
    "ws": "^7.3.1"
  },
  "devDependencies": {
    "eslint": "^7.8.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-streamr-nodejs": "^1.3.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-import-order": "2.1.4",
    "eslint-plugin-promise": "^4.2.1",
